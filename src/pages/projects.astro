---
import BaseLayout from "@layouts/BaseLayout.astro";
import HorizontalCard from "@components/HorizontalCard.astro";
---

<BaseLayout title="Proyectos">
  <div>
    <div class="text-3xl w-full font-bold mb-5 mt-10"></div>
  </div>

  <HorizontalCard
    title="Custom Electron Titlebar"
    img="/images/cet.webp"
    desc="Librería para personalizar la barra de titulo de una aplicación electron al estilo VS Code, que es 100% personalizable."
    url="https://www.npmjs.com/package/custom-electron-titlebar"
    badge="Activo"
  />
</BaseLayout>
