{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "./src",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/layouts/*": [
        "layouts/*"
      ],
      "@/public/*": [
        "public/*"
      ],
      "@/data/*": [
        "data/*"
      ]
    }
  }
}